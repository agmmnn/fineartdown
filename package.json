{
  "name": "fineartdown",
  "version": "1.1.0",
  "description": "Download full size images from Fine Art America, Conde Nast, Photos.com, Pixels.com",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "agmmnn",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "prepublish": "tsc",
    "fineartdown": "tsc | node ./dist/main.js"
  },
  "bin": {
    "fineartdown": "./cli/index.js"
  },
  "keywords": [
    "Fine Art America",
    "fineartamerica.com",
    "condenaststore.com",
    "Photos.com",
    "Pixels.com",
    "downloader",
    "image"
  ],
  "files": [
    "cli/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agmmnn/fineartdown.git"
  },
  "bugs": {
    "url": "https://github.com/agmmnn/fineartdown/issues"
  },
  "dependencies": {
    "canvas": "^2.11.0",
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "typescript": "^4.9.5"
  },
  "pkg": {
    "scripts": [
      "cli/index.js",
      "dist/**/*.js"
    ],
    "assets": "node_modules/canvas/**/*",
    "targets": [
      "win",
      "macos",
      "linux"
    ],
    "outputPath": "exec"
  }
}
